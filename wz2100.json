{
  "buildinfo": [
    {
      "prerun": [
	"curl -sSL -o ~/.cache/deb2appimage/debs/warzone2100_ubuntu22.04_amd64.deb 'https://downloads.sourceforge.net/project/warzone2100/releases/4.5.5/warzone2100_ubuntu22.04_amd64.deb'",
        "mkdir -p ~/.cache/deb2appimage/AppDir/usr/bin",
        "cp ./startup.sh ~/.cache/deb2appimage/AppDir/usr/bin/warzone2100.sh"
      ],
      "name": "warzone2100",
      "version": "4.5.5-glibc_2.35",
      "deps": "libasound2,libcurl3-gnutls,libdecor-0-0,libdrm2,libfreetype6,libfribidi0,libgbm1,libgcc-s1,libharfbuzz0b,libogg0,libopenal1,libopus0,libphysfs1,libpng16-16,libsodium23,libsqlite3-0,libtheora0,libvorbis0a,libvorbisfile3,zlib1g,librtmp1,libssh2-1,libldap-2.5-0,libsndio7.0,libsasl2-2,libbsd0,libmd0,libnghttp2-14",
      "repoarch": "amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64,amd64",
      "distrorepo": "Debian",
      "repoversion": "bookworm",
      "binarypath": "/usr/bin/warzone2100",
      "desktoppath": "/usr/share/applications/net.wz2100.warzone2100.desktop",
      "iconpath": "/usr/share/icons/net.wz2100.warzone2100.png",
      "usewrapper": "false",
      "postrun": [
      ]
    }
  ],
  "apprunconf": [
    {
      "setpath": "true",
      "setlibpath": "true",
      "setpythonpath": "false",
      "setpythonhome": "false",
      "setpythondontwritebytecode": "false",
      "setxdgdatadirs": "false",
      "setperllib": "false",
      "setgsettingsschemadir": "false",
      "setqtpluginpath": "false",
      "exec": "/usr/bin/warzone2100.sh"
    }
  ],
  "authors": [
    {
      "type": "Author",
      "author": "Warzone2100 contributors",
      "url": "https://wz2100.net"
    },
    {
      "type": "AppImage Maintainer",
      "author": "pbakota",
      "url": "https://github.com/pbakota/warzone2100-appimage"
    }
  ]
}

